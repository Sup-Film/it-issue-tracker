# Copy this file to .env and fill values before running docker-compose or local dev
# Do NOT commit real secrets. Keep .env in .gitignore.

# Database connection used by Prisma and the backend app.
# Example (when using docker-compose):
# DATABASE_URL="postgresql://postgres:1234@db:5432/it_issue_tracker?schema=public"
DATABASE_URL="postgresql://<DB_USER>:<DB_PASS>@<DB_HOST>:<DB_PORT>/<DB_NAME>?schema=public"

# JWT secret used to sign access tokens. Use a strong random string.
JWT_SECRET=<replace-with-strong-random-secret>
